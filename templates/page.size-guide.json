  const contents = document.querySelectorAll(\".tab-content\");\n        const unitToggle = document.querySelectorAll('input[name=\"unit\"]');\n        \n        \/\/ Tab Switching\n        tabs.forEach(tab => {\n            tab.addEventListener(\"click\", () => {\n            \/\/ Remove active class from previous tab\n            document.querySelector(\".tab-button.active\").classList.remove(\"active\");\n            tab.classList.add(\"active\");\n        \n            \/\/ Hide previous content and show current tab's content\n            document.querySelector(\".tab-content.active\").classList.remove(\"active\");\n            document.getElementById(tab.dataset.tab).classList.add(\"active\");\n        \n            \/\/ Trigger unit update for the newly active tab\n            updateUnits();\n            });\n        });\n        \n        \/\/ Unit Switching\n        unitToggle.forEach(toggle => {\n            toggle.addEventListener(\"change\", updateUnits);\n        });\n        \n        \/\/ Function to update table units (cm\/inch)\n        function updateUnits() {\n            const selectedUnit = document.querySelector('input[name=\"unit\"]:checked').value;\n        \n            \/\/ Update all visible tables in the active tab\n            document.querySelectorAll(\".tab-content.active table tbody tr td\").forEach(cell => {\n            if (cell.dataset[selectedUnit]) {\n                cell.textContent = cell.dataset[selectedUnit];\n            }\n            });\n        }\n        \n        \/\/ Initialize units on page load\n        updateUnits();\n        });\n        \n        <\/script>\n\n\n\n\n\n    <style>\n        .measurement-section {\n        font-family: Arial, sans-serif;\n        }\n        \n        label {\n        display: flex;\n        gap: 10px;\n        }\n        \n        input.unit {\n        display: block;\n        }\n        \n        .unit-toggle {\n        margin-bottom: 20px;\n        }\n        \n        .tabs {\n        display: flex;\n        }\n        \n        .tab-button {\n        height: 45px;\n        width: 20%;\n        margin-right: 5px;\n        border: none;\n        cursor: pointer;\n        background-color: #ffffff;\n        }\n        \n        .tab-button.active {\n            background-color: #ededed;}\n        \n        .tab-content {\n        display: none;\n        border: 1px solid #bdbdbd;\n        padding: 20px;\n        }\n        .images {\n            display: flex;\n            margin: 30px 0;\n        }\n        img.img {\n            width: 20%;\n        }\n        .tab-content.active {\n        display: block;\n        }\n        \n        .table {\n        width: 60vw;\n        margin: auto;\n        }\n        \n        table {\n        width: 100%;\n        border-collapse: collapse; \/* Ensures proper table structure *\/\n        }\n        \n        table th, table td {\n            line-height: 18px;\n            text-align: center;\n            font-size: 14px;\n            font-family: 'Archivo-Regular';\n            font-weight: 400;\n        }\n        \n        thead th {\n            line-height: 26px;\n        }\n        \n        table tbody tr:nth-child(odd) {\n        background-color: white; \/* White background for odd rows *\/\n        }\n        \n        table tbody tr:nth-child(even) {\n        background-color: lightgray; \/* Gray background for even rows *\/\n        }\n        h3 {\n            font-size: 14px;\n            font-family: 'Prata-Regular';\n            font-weight: 700;\n        }\n        \n@media (max-width:1024px){\n  .table{\n    width:90vw !important;\n  }\n}\n    <\/style>"}}},"order":["custom_liquid_DmrBpj","main","custom_liquid_dDhetR"]}